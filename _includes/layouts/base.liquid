{%- capture distFolder %}/web/dist/{% if distVersion %}{{ distVersion }}{% else %}{{ pkg.version }}{% endif %}/{% endcapture -%}
{%- capture fullUrl %}https://www.zachleat.com{{ page.url }}{% endcapture -%}
<!doctype html>
<html lang="en"{% if tmpl or templateclass %} class="{% if tmpl %} tmpl-{{ tmpl }}{% endif %}{% if templateclass %} {{ templateclass }}{% endif %}"{% endif %}>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">

		<title>{% if title %}{% if titleOverride %}{{ titleOverride }}{% else %}{% if renderData.title %}{{ renderData.title }}{% else %}{{ title }}{% endif %}{% endif %}—zachleat.com{% else %}zachleat.com{% endif %}</title>

		<link rel="preload" href="/web/css/fonts/lato/2.0/latolatin-regular-subset.woff2" as="font" type="font/woff2" crossorigin>

		<meta name="author" content="Zach Leatherman">
		<meta name="description" content="{% if description %}{{ description }}, {% endif %}{% if title %}A post by {% endif %}Zach Leatherman (zachleat)">
		<meta name="generator" content="{{ eleventy.generator }}">

		<!-- Icons -->
		<link rel="apple-touch-icon-precomposed" href="/img/rel-icon-192.png">
		<link rel="icon" href="/img/rel-icon-192.jpg" sizes="192x192" type="image/jpeg">

		<!-- Social -->
		<meta property="og:title" content="{% if title %}{{ title }}—zachleat.com{% else %}zachleat.com{% endif %}">
		<meta property="og:type" content="website">
		<meta property="og:site_name" content="Zach Leatherman">
		<meta property="og:description" content="{% if description %}{{ description }}, {% endif %}{% if title %}A post by {% endif %}Zach Leatherman (zachleat)">
		<meta property="og:image" content="{% if headimage %}{{ headimage | absoluteUrl }}{% else %}{% ogImageSource page %}{% endif %}">
		<meta property="og:url" content="https://www.zachleat.com{{ page.url }}">
		<meta name="twitter:card" content="summary_large_image" />
		<meta name="twitter:site" content="@zachleat" />
		<meta name="twitter:image" content="{% if headimage %}{{ headimage | absoluteUrl }}{% else %}{% ogImageSource page %}{% endif %}" />

		<!-- Site-specific -->
		<meta name="dist" content="{{distFolder}}">

		<!-- SEO/Feeds -->
		<link rel="canonical" href="{{ fullUrl }}">
{% if rssFeedUrl and rssFeedTitle %}
		<link rel="alternate" href="{{ rssFeedUrl }}" title="{{ rssFeedTitle }}" type="application/atom+xml">
{% else %}
		<link rel="alternate" href="/web/feed/" title="Zach Leatherman’s Posts Feed" type="application/atom+xml">
		<link rel="alternate" href="/follow/" title="Zach Leatherman’s Activity Feed" type="application/atom+xml">
{% endif %}
		<link rel="feed" href="https://www.zachleat.com/web/">

		<style>{% include "initial.min.css" %}</style>

		<!-- Preconnect to Eleventy services -->
		<link href="https://v1.opengraph.11ty.dev" rel="preconnect" crossorigin>
		<link href="https://v1.indieweb-avatar.11ty.dev" rel="preconnect" crossorigin>

		<link rel="stylesheet" href="{{ distFolder }}defer.min.css" media="print" onload="this.media='all'">
		<noscript>
			<link href="{{ distFolder }}defer.min.css" rel="stylesheet">
		</noscript>
{%- for url in stylesheets -%}
		<link href="{{ url }}" rel="stylesheet">
{%- endfor %}
{%- for stylesheet in stylesheetsDist -%}
		<link href="{{ distFolder }}{{ stylesheet }}" rel="stylesheet">
{%- endfor %}
		<script>{% include "initial.min.js" %}</script>
		<script src="{{ distFolder }}defer-mod.min.js" type="module" defer></script>
		<script src="{{ distFolder }}defer.min.js" defer></script>

		<meta property="eleventy:path" content="{{ page.inputPath }}">

		<!-- IndieAuth -->
		<link href="mailto:zach@zachleat.com" rel="me">
		<link href="https://twitter.com/zachleat" rel="me">
		<link href="https://github.com/zachleat" rel="me">
		<link href="https://zachleat.com/@zachleat" rel="me">
		<link href="https://fediverse.zachleat.com/@zachleat" rel="me">
{% if enableWebmentions %}
		<link rel="webmention" href="https://webmention.io/www.zachleat.com/webmention" />
		<link rel="pingback" href="https://webmention.io/www.zachleat.com/xmlrpc" />
{% endif %}
	</head>
	<body class="
			{%- if classes and classes.body %} {{ classes.body }}{% endif -%}
			{%- if seo and seo.openGraphSkipPageBackground %}{% else %}{% if seo and seo.openGraphBackgroundImage %} body-hasbgimage{% endif %}{% endif -%}
		"
		{% if seo and seo.openGraphSkipPageBackground %}{% else %}{% if seo and seo.openGraphBackgroundImage %} style="--seo-bgimage: {{ seo.openGraphBackgroundImage | backgroundimage: 1200 }}"{% endif %}{% endif %}>
		{{ content }}
	</body>
</html>