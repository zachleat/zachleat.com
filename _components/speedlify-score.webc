<is-land on:visible>
	<style>
	speedlify-score {
		font-style: normal;
		font-size: 0.9375em; /* 15px /16 */
	}
	</style>

	<speedlify-score webc:raw webc:root @attributes speedlify-url="https://www.speedlify.dev"></speedlify-score>

	<template data-island="once" webc:raw>
		<script src="/static/speedlify-score.js"></script>
	</template>
</is-land>