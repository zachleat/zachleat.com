	"scripts": {
		"clean": "rm -rf _site && rm -rf .cache",
		"check-links": "npx check-html-links _site",
		"create-search-index": "npx pagefind --site \"_site\" --glob \"{web,resume}/**/*.html\"",
		"build": "npx @11ty/eleventy",
		"start": "npx @11ty/eleventy --serve --port=8090 --incremental",
		"start-search": "npm run build && npm run create-search-index && npm run start",
		"projects": "./fetch-projects.sh",
		"analytics": "node fetch-analytics-data.js",
		"production": "npm run projects && npm run analytics && npm run build-production && npm run create-search-index",
		"nocache-production": "npm run projects && npm run build-production && npm run create-search-index",
		"build-production": "PRODUCTION_BUILD=true npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
